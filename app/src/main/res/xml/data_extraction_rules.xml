<?xml version="1.0" encoding="utf-8"?>
<!-- Android 12+ (API 31+) data extraction rules.
     These control what data is included in cloud and device-to-device transfer. -->
<data-extraction-rules>
    <cloud-backup>
        <include domain="database" path="offline_video_player.db" />
        <include domain="sharedpref" path="." />
    </cloud-backup>
    <device-transfer>
        <include domain="database" path="offline_video_player.db" />
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>
